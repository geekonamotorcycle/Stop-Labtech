Clear-Host; Write-Output "WARNING: Ensure the Labtech service monitor is running.`notherwise it will not restart on its own"; Read-Host "Press enter to being"; $ProcID = Get-Process | Where-Object -Property ProcessName -EQ "ltsvc" | Select-Object Processname,ID; if($ProcID){Write-Output "Found The process with ID "$ProcId.id""; Stop-Process -Id $ProcID.ID -force}else{Write-Output "The Process Could not be found."};
